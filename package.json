{
  "name": "@insomnia-dev/words-to-numbers",
  "version": "2.1.2",
  "description": "Convert textual words to numbers with optional fuzzy text matching",
  "license": "MIT",
  "author": "Chris Sandvik <chris.sandvik@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/insomnia-dev/words-to-numbers.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "lint": "npm run lint:src && npm run lint:types && npm run lint:exports",
    "lint:exports": "attw --pack .",
    "lint:src": "eslint .",
    "lint:types": "tsc",
    "prepublishOnly": "npm run build && npm run lint",
    "postpublish": "git push --follow-tags",
    "test": "vitest ./src"
  },
  "dependencies": {
    "@skyra/jaro-winkler": "^1.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
